{
  "name": "wdio-mobile-test",
  "type": "module",
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@wdio/allure-reporter": "^9.20.0",
    "@wdio/appium-service": "^9.20.0",
    "@wdio/browserstack-service": "^9.20.0",
    "@wdio/cli": "^9.20.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "appium-uiautomator2-driver": "^3.10.0",
    "dotenv": "^17.2.3"
  },
  "scripts": {
    "test": "wdio run ./wdio.s22.conf.js",
    "test:pixel": "wdio run ./wdio.pixel7.conf.js",
    "test:local": "wdio run ./wdio.local.conf.js",
    "test:formsPage": "wdio run ./wdio.s22.conf.js --spec formsPage.specs.js",
    "test:local:formsPage": "wdio run ./wdio.local.conf.js --spec formsPage.specs.js",
    "test:signUpTab": "wdio run ./wdio.s22.conf.js --spec signUpTab.specs.js",
    "test:local:signUpTab": "wdio run ./wdio.local.conf.js --spec signUpTab.specs.js",
    "test:loginTab": "wdio run ./wdio.s22.conf.js --spec loginTab.specs.js",
    "test:local:loginTab": "wdio run ./wdio.local.conf.js --spec loginTab.specs.js",
    "test:dragPage": "wdio run ./wdio.s22.conf.js --spec dragPage.specs.js",
    "test:local:dragPage": "wdio run ./wdio.local.conf.js --spec dragPage.specs.js",
    "test:webviewPage": "wdio run ./wdio.s22.conf.js --spec webviewPage.specs.js",
    "test:local:webviewPage": "wdio run ./wdio.local.conf.js --spec webviewPage.specs.js",
    "test:swipePage": "wdio run ./wdio.s22.conf.js --spec swipePage.specs.js",
    "test:local:swipePage": "wdio run ./wdio.local.conf.js --spec swipePage.specs.js",
    "report": "allure generate allure-results --clean && allure open"
  }
}
